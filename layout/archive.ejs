<div class="page">
  <div class="lead">
    <div class="container">
      <h1><%= __('archives') %></h1>
    </div>
  </div>
  <div class="container">
    <div class="main-content">
      <%- partial('@/widgets/archive_map') %>
      <ul class="post-list">
        <% page.posts.forEach(function(post) { %>
          <%- partial('@/post-item', {post: post}) %>
        <% }) %>
      </ul>
      <%- partial('@/paginator')%>
    </div>
    <div class="sidebar">
      <%- partial('@/asides/recent_posts')%>
      <%- partial('@/asides/categories')%>
      <%- partial('@/asides/tagcloud')%>
    </div>
  </div>
</div>